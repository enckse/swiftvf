{
  "clock": {
    "deadline": 10,
    "port": 1234
  },
  "boot": {
    "efi": {
      "store": "build/efi_vars.fd"
    }
  },
  "graphics": {
      "height": 600,
      "width": 800
  },
  "identifier": "build/machine_id",
  "resources": {
      "cpus": 1,
      "memory": 256
  },
  "cmdline": "myargs",
  "serial": "none",
  "disks": [
    { 
      "mode": "block",
      "path": "build/apkovl.img"
    },
    { 
      "mode": "block",
      "path": "$BUILD/data.img",
      "readonly": false
    }
  ],
  "env": {
      "BUILD": "build"
  }
}
